<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>打包游戏任务</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>ProjectPath</name>
          <description>项目路径</description>
          <defaultValue>G:\Work\unity2021315f1pj\F8Framework</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>Platform</name>
          <description>打包平台</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>StandaloneWindows64</string>
              <string>StandaloneWindows</string>
              <string>Android</string>
              <string>iOS</string>
              <string>WebGL</string>
              <string>StandaloneOSX</string>
              <string>StandaloneLinux64</string>
              <string>WSAPlayer</string>
              <string>tvOS</string>
              <string>VisionOS</string>
              <string>PS4</string>
              <string>PS5</string>
              <string>Switch</string>
              <string>XboxOne</string>
              <string>GameCoreXboxSeries</string>
              <string>GameCoreXboxOne</string>
              <string>LinuxHeadlessSimulation</string>
              <string>EmbeddedLinux</string>
              <string>QNX</string>
              <string>ReservedCFE</string>
              <string>Kepler</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>BuildPath</name>
          <description>打包输出目录</description>
          <defaultValue>G:\Work\unity2021315f1pj\F8Framework\Builds</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>Version</name>
          <description>版本号</description>
          <defaultValue>1.0.0</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>CodeVersion</name>
          <description>构建次数，某些平台需要递增</description>
          <defaultValue>1</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ExcelPath</name>
          <description>设置Excel存放目录</description>
          <defaultValue>G:\Work\unity2021315f1pj\F8Framework\Assets\StreamingAssets\config</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ExcelBinDataFolder</name>
          <description>设置导出配置表目录（注意：每次导表会清空该目录）</description>
          <defaultValue>G:\Work\unity2021315f1pj\F8Framework\Assets\AssetBundles\Config\BinConfigData</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>ConvertExcelToOtherFormats</name>
          <description>选择配置表格式</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>binary</string>
              <string>json</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableFullPathAssetLoading</name>
          <description>启用完整资源路径加载</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableFullPathExtensionAssetLoading</name>
          <description>启用完整资源路径带扩展名</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>AppendHashToAssetBundleName</name>
          <description>打包后AB名加上MD5</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>ForceRemoteAssetBundle</name>
          <description>强制更改资产加载模式为远程</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>DisableUnityCacheOnWebGL</name>
          <description>禁用Unity缓存系统在WebGL平台</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AssetBundleOffset</name>
          <description>AssetBundle 偏移加密（Offset）  [1-254]</description>
          <defaultValue>0</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AssetBundleXorKey</name>
          <description>AssetBundle 异或加密（XOR）    [1-254]</description>
          <defaultValue>0</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AssetRemoteAddress</name>
          <description>资产远程地址/游戏远程版本</description>
          <defaultValue>http://127.0.0.1:6789/</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableHotUpdate</name>
          <description>启用热更新</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnablePackage</name>
          <description>启用分包</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>OptionalPackagePassword</name>
          <description>分包zip加密</description>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>ForceRebuildAssetBundle</name>
          <description>强制重新构建AB包</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>CleanBuildCache</name>
          <description>强制清理打包缓存</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableFullPackage</name>
          <description>全量资源打包进游戏</description>
          <defaultValue>true</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableOptionalPackage</name>
          <description>打包分包游戏</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>OptionalPackage</name>
          <description>选择分包资源包，分隔符：下划线</description>
          <defaultValue>0_1_2_3</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>EnableNullPackage</name>
          <description>打空包游戏</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>AndroidBuildAppBundle</name>
          <description>构建aab包（Google Play），安卓专用，aab包资源超过 150 MB 后的解决方案：https://github.com/google/play-appbundle-unity/releases</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>AndroidUseKeystore</name>
          <description>使用自定义签名，安卓专用</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AndroidKeystoreName</name>
          <description>选择Keystore文件路径（包含后缀），安卓专用</description>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.PasswordParameterDefinition>
          <name>AndroidKeystorePass</name>
          <description>Keystore密码，安卓专用</description>
          <defaultValue>{AQAAABAAAABACJf+0pjTWsS/u9SV+7AFx/iubQVs9DtjiHf1JfcvjIGmBucXIOgmeJak0jc7/fFzTZdHKCUgygRaOyVuU50To49QrluRmGYbxfeX85lek7Q=}</defaultValue>
        </hudson.model.PasswordParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AndroidKeyAliasName</name>
          <description>密钥别名，安卓专用</description>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.PasswordParameterDefinition>
          <name>AndroidKeyAliasPass</name>
          <description>别名密码，安卓专用</description>
          <defaultValue>{AQAAABAAAABAAGqWauqjWOtaUVG6CMZOuvC6vzNwA4geGYSv6oDK5gThMA7WB2WCZohMACUF+69w/+ru1XuMv88F708iZYE0vHmhUa6ylrm3RPszz+0AfiY=}</defaultValue>
        </hudson.model.PasswordParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <org.jenkinsci.plugins.unity3d.Unity3dBuilder plugin="unity3d-plugin@1.3">
      <unity3dName>unity2021.3.15</unity3dName>
      <argLine>-quit -nographics -batchmode -projectPath $ProjectPath -executeMethod F8Framework.Core.Editor.ExcelDataTool.JenkinsLoadAllExcelData -logFile $BuildPath\JenkinsBuildUnity.log -buildTarget $Platform ExcelPath- $ExcelPath ConvertExcelToOtherFormats- $ConvertExcelToOtherFormats ExcelBinDataFolder- $ExcelBinDataFolder</argLine>
      <unstableReturnCodes></unstableReturnCodes>
    </org.jenkinsci.plugins.unity3d.Unity3dBuilder>
    <org.jenkinsci.plugins.unity3d.Unity3dBuilder plugin="unity3d-plugin@1.3">
      <unity3dName>unity2021.3.15</unity3dName>
      <argLine>-quit -nographics -batchmode -projectPath $ProjectPath -executeMethod F8Framework.Core.Editor.ExcelDataTool.JenkinsAllScriptsReloaded -logFile $BuildPath\JenkinsBuildUnity.log -buildTarget $Platform ExcelPath- $ExcelPath ConvertExcelToOtherFormats- $ConvertExcelToOtherFormats ExcelBinDataFolder- $ExcelBinDataFolder</argLine>
      <unstableReturnCodes></unstableReturnCodes>
    </org.jenkinsci.plugins.unity3d.Unity3dBuilder>
    <org.jenkinsci.plugins.unity3d.Unity3dBuilder plugin="unity3d-plugin@1.3">
      <unity3dName>unity2021.3.15</unity3dName>
      <argLine>-quit -nographics -batchmode -projectPath $ProjectPath -executeMethod F8Framework.Core.Editor.F8Helper.GenerateCopyHotUpdateDll -logFile $BuildPath\JenkinsBuildUnity.log -buildTarget $Platform</argLine>
      <unstableReturnCodes></unstableReturnCodes>
    </org.jenkinsci.plugins.unity3d.Unity3dBuilder>
    <org.jenkinsci.plugins.unity3d.Unity3dBuilder plugin="unity3d-plugin@1.3">
      <unity3dName>unity2021.3.15</unity3dName>
      <argLine>-quit -nographics -batchmode -projectPath $ProjectPath -executeMethod F8Framework.Core.Editor.ABBuildTool.JenkinsBuildAllAB -logFile $BuildPath\JenkinsBuildUnity.log -buildTarget $Platform EnableFullPathAssetLoading- $EnableFullPathAssetLoading EnableFullPathExtensionAssetLoading- $EnableFullPathExtensionAssetLoading ForceRebuildAssetBundle- $ForceRebuildAssetBundle AppendHashToAssetBundleName- $AppendHashToAssetBundleName ForceRemoteAssetBundle- $ForceRemoteAssetBundle DisableUnityCacheOnWebGL- $DisableUnityCacheOnWebGL AssetBundleOffset- $AssetBundleOffset AssetBundleXorKey- $AssetBundleXorKey</argLine>
      <unstableReturnCodes></unstableReturnCodes>
    </org.jenkinsci.plugins.unity3d.Unity3dBuilder>
    <org.jenkinsci.plugins.unity3d.Unity3dBuilder plugin="unity3d-plugin@1.3">
      <unity3dName>unity2021.3.15</unity3dName>
      <argLine>-quit -nographics -batchmode -projectPath $ProjectPath -executeMethod F8Framework.Core.Editor.BuildPkgTool.JenkinsBuild -logFile $BuildPath\JenkinsBuildUnity.log -buildTarget $Platform Platform- $Platform BuildPath- $BuildPath Version- $Version CodeVersion- $CodeVersion AssetRemoteAddress- $AssetRemoteAddress EnableHotUpdate- $EnableHotUpdate EnablePackage- $EnablePackage EnableFullPackage- $EnableFullPackage EnableOptionalPackage- $EnableOptionalPackage OptionalPackage- $OptionalPackage EnableNullPackage- $EnableNullPackage AndroidBuildAppBundle- $AndroidBuildAppBundle AndroidUseKeystore- $AndroidUseKeystore AndroidKeystoreName- $AndroidKeystoreName AndroidKeystorePass- $AndroidKeystorePass AndroidKeyAliasName- $AndroidKeyAliasName AndroidKeyAliasPass- $AndroidKeyAliasPass CleanBuildCache- $CleanBuildCache OptionalPackagePassword- $OptionalPackagePassword</argLine>
      <unstableReturnCodes></unstableReturnCodes>
    </org.jenkinsci.plugins.unity3d.Unity3dBuilder>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>